:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x137e07400 .scope module, "bin2bcd_tb" "bin2bcd_tb" 2 3;
 .timescale -9 -12;
v0x137e19890_0 .net "bcd", 7 0, v0x137e04c90_0;  1 drivers
v0x137e19950_0 .var "bin", 7 0;
S_0x137e04b20 .scope module, "dut" "bin2bcd" 2 7, 3 1 0, S_0x137e07400;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "bin";
    .port_info 1 /OUTPUT 8 "bcd";
v0x137e04c90_0 .var "bcd", 7 0;
v0x137e19710_0 .net "bin", 7 0, v0x137e19950_0;  1 drivers
v0x137e197b0_0 .var/i "i", 31 0;
E_0x137e07570 .event anyedge, v0x137e19710_0;
    .scope S_0x137e04b20;
T_0 ;
    %wait E_0x137e07570;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x137e04c90_0, 0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x137e197b0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x137e197b0_0;
    %cmpi/s 7, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x137e04c90_0;
    %parti/s 4, 0, 2;
    %pad/u 32;
    %cmpi/u 5, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.2, 5;
    %load/vec4 v0x137e04c90_0;
    %parti/s 4, 0, 2;
    %addi 3, 0, 4;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x137e04c90_0, 4, 4;
T_0.2 ;
    %load/vec4 v0x137e04c90_0;
    %parti/s 4, 4, 4;
    %pad/u 32;
    %cmpi/u 5, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.4, 5;
    %load/vec4 v0x137e04c90_0;
    %parti/s 4, 4, 4;
    %addi 3, 0, 4;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x137e04c90_0, 4, 4;
T_0.4 ;
    %load/vec4 v0x137e04c90_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x137e19710_0;
    %pushi/vec4 7, 0, 32;
    %load/vec4 v0x137e197b0_0;
    %sub;
    %part/s 1;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x137e04c90_0, 0, 8;
    %load/vec4 v0x137e197b0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x137e197b0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x137e07400;
T_1 ;
    %vpi_call 2 13 "$dumpfile", "./bin2bcd.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars" {0 0 0};
    %pushi/vec4 36, 0, 8;
    %assign/vec4 v0x137e19950_0, 0;
    %delay 10000, 0;
    %pushi/vec4 27, 0, 8;
    %assign/vec4 v0x137e19950_0, 0;
    %end;
    .thread T_1;
    .scope S_0x137e07400;
T_2 ;
    %delay 20000, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_2;

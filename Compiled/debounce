:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x122706450 .scope module, "debounce_tb" "debounce_tb" 2 3;
 .timescale -9 -12;
P_0x122706b40 .param/l "CLK_PERIOD" 0 2 14, +C4<00000000000000000000000000001010>;
v0x12271b260_0 .var "clk", 0 0;
v0x12271b300_0 .net "debounced_signal", 0 0, v0x12271b020_0;  1 drivers
v0x12271b3b0_0 .var "signal", 0 0;
S_0x122705000 .scope module, "dut" "debounce" 2 8, 3 1 0, S_0x122706450;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "signal";
    .port_info 2 /OUTPUT 1 "debounced_signal";
v0x12270b2a0_0 .net "clk", 0 0, v0x12271b260_0;  1 drivers
v0x12271b020_0 .var "debounced_signal", 0 0;
v0x12271b0c0_0 .var "delayed_signal", 0 0;
v0x12271b170_0 .net "signal", 0 0, v0x12271b3b0_0;  1 drivers
E_0x1227071c0 .event posedge, v0x12270b2a0_0;
    .scope S_0x122705000;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12271b0c0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x122705000;
T_1 ;
    %wait E_0x1227071c0;
    %load/vec4 v0x12271b170_0;
    %assign/vec4 v0x12271b0c0_0, 0;
    %load/vec4 v0x12271b0c0_0;
    %assign/vec4 v0x12271b020_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x122706450;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12271b260_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12271b3b0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x122706450;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x12271b260_0;
    %inv;
    %store/vec4 v0x12271b260_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x122706450;
T_4 ;
    %vpi_call 2 19 "$dumpfile", "./debounce.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars" {0 0 0};
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12271b3b0_0, 0, 1;
    %delay 3000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12271b3b0_0, 0, 1;
    %delay 3000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x12271b3b0_0, 0, 1;
    %delay 3000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12271b3b0_0, 0, 1;
    %delay 30000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x12271b3b0_0, 0, 1;
    %end;
    .thread T_4;
    .scope S_0x122706450;
T_5 ;
    %delay 50000, 0;
    %vpi_call 2 33 "$finish" {0 0 0};
    %end;
    .thread T_5;
